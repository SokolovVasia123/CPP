#include <iostream>

using namespace std;

int main()
{
    setlocale(0,"Russian");
    int a;
    float b;
    char c=-32;
    bool d=true;
    cout << "Введите целое число" << endl;
    cin >> a;
    cout << "Введите число c плавающей запятой" << endl;
    cin >> b;
    cout << c+c+c << endl;
    cout << a/b << endl;
    cout << a-b << endl;
    cout << a*a << endl;
    return 0;
}
